{
	"name": "interest-page",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 5111",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"postinstall": "svelte-kit sync && prisma generate",
		"lint": "prettier --check . && eslint .",
		"slice": "start-slicemachine",
		"vercel-build": "prisma generate && prisma db push  && vite build"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@resvg/resvg-js": "^2.6.2",
		"@slicemachine/adapter-sveltekit": "^0.3.90",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/canvas-confetti": "^1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"prisma": "^6.19.0",
		"satori": "^0.18.3",
		"satori-html": "^0.3.2",
		"slice-machine-ui": "^2.21.1",
		"svelte": "^5.50.1",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^0.2.1"
	},
	"dependencies": {
		"@neondatabase/serverless": "^1.0.1",
		"@prisma/client": "^6.19.0",
		"@prismicio/client": "^7.21.0",
		"@prismicio/svelte": "^2.0.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@vercel/og": "^0.8.6",
		"@vercel/speed-insights": "^1.3.1",
